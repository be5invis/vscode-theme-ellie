	"tokenColors": [
		{
			"name": "Identifier",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"variable.other.readwrite",
				"variable.other.constant",
				"variable.other.readonly"
			],
			"settings": {
				"foreground": "#3B3035"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
				"variable.other.local",
				"variable.other.constant"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3B3035"
			}
		},
		{
			"name": "Member Access",
			"scope": [
				"variable.other.property",
				"variable.other.constant.property"
			],
			"settings": {
				"foreground": "#C25100"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#3B3035"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#827279"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#9F1080"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#827279"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php",
				"punctuation.definition.typeparameters"
			],
			"settings": {
				"foreground": "#DB7200"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9E5DE1"
			}
		},
		{
			"scope": [
				"meta.template.expression.ts"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"constant.dec.numeric",
				"constant.hex.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#8639CB"
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.namespace",
				"entity.name.type.module"
			],
			"settings": {
				"foreground": "#B54000"
			}
		},
		{
			"name": "Type name",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#B93E98"
			}
		},
		{
			"name": "Built-in type name",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#AC2A8C"
			}
		},
		{
			"name": "Type parameter",
			"scope": [
				"entity.name.type.parameter"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C25100"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#8639CB"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name",
				"entity.name.class",
				"entity.name.function",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"string.key",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"entity.name.function.member"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Weak Keyword",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#120B0E"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9E1182"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9E1182"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#D25553"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#D25553"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9E1182"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"foreground": "#C25100"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Markdown link URL",
			"scope": [
				"markup.underline.link.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9E5DE1"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "Makefile leaders",
			"scope": [
				"punctuation.definition.markdown",
				"punctuation.definition.heading.markdown",
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.quote.begin.markdown"
			],
			"settings": {
				"foreground": "#9F1080"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#261D21"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9E1182"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#261D21"
			}
		}
	],
	"semanticHighlighting": true
}