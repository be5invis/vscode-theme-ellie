		"breadcrumb.foreground": "#767171",
		"breadcrumb.focusForeground": "#A793D5",
		"breadcrumb.activeSelectionForeground": "#A793D5",
		"breadcrumbPicker.background": "#282423",
		"terminal.background": "#171212",
		"terminal.foreground": "#B8B3B2",
		"terminal.cursorColor": "#FFCCFC",
		"terminal.selectionBackground": "#6C495E",
		"terminal.ansiBlack": "#171212",
		"terminal.ansiBrightBlack": "#615C5C",
		"terminal.ansiWhite": "#B8B3B2",
		"terminal.ansiBrightWhite": "#E7E1E1",
		"terminal.ansiRed": "#D2647D",
		"terminal.ansiYellow": "#938933",
		"terminal.ansiGreen": "#259969",
		"terminal.ansiCyan": "#009AB9",
		"terminal.ansiBlue": "#5588D7",
		"terminal.ansiMagenta": "#9A78C6",
		"terminal.ansiBrightRed": "#FFA1BD",
		"terminal.ansiBrightYellow": "#DBCD65",
		"terminal.ansiBrightGreen": "#5FE2A5",
		"terminal.ansiBrightCyan": "#00E2FF",
		"terminal.ansiBrightBlue": "#94CBFF",
		"terminal.ansiBrightMagenta": "#E3B8FF"
	},
	"tokenColors": [
		{
			"name": "Identifier",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"variable.other.readwrite",
				"variable.other.constant",
				"variable.other.readonly"
			],
			"settings": {
				"foreground": "#B8B3B2"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
				"variable.other.local",
				"variable.other.constant"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#B8B3B2"
			}
		},
		{
			"name": "Member Access",
			"scope": [
				"variable.other.property",
				"variable.other.constant.property"
			],
			"settings": {
				"foreground": "#E3A871"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#B8B3B2"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#767171"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#FFCCFC"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#767171"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php",
				"punctuation.definition.typeparameters"
			],
			"settings": {
				"foreground": "#B27C47"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A793D5"
			}
		},
		{
			"scope": [
				"meta.template.expression.ts"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"constant.dec.numeric",
				"constant.hex.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#D5C0FF"
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.namespace",
				"entity.name.type.module"
			],
			"settings": {
				"foreground": "#FCBF87"
			}
		},
		{
			"name": "Type name",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#E79DCD"
			}
		},
		{
			"name": "Built-in type name",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#FFB4E4"
			}
		},
		{
			"name": "Type parameter",
			"scope": [
				"entity.name.type.parameter"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E3A871"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#D5C0FF"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name",
				"entity.name.class",
				"entity.name.function",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"string.key",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"entity.name.function.member"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Weak Keyword",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#E7E1E1"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E7E1E1"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E7E1E1"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#E98068"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#E98068"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E7E1E1"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"foreground": "#E3A871"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Markdown link URL",
			"scope": [
				"markup.underline.link.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A793D5"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "Makefile leaders",
			"scope": [
				"punctuation.definition.markdown",
				"punctuation.definition.heading.markdown",
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.quote.begin.markdown"
			],
			"settings": {
				"foreground": "#FFCCFC"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E7E1E1"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#CFCAC9"
			}
		}
	],
	"semanticHighlighting": true
}