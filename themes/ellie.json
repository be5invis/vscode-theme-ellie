		"list.focusBackground": "#A25A8A40",
		"list.inactiveSelectionForeground": "#E7E1E1",
		"list.activeSelectionForeground": "#E7E1E1",
		"list.focusForeground": "#E7E1E1",
		"list.errorForeground": "#FF957B",
		"list.warningForeground": "#C2B654",
		"gitDecoration.modifiedResourceForeground": "#00CAED",
		"gitDecoration.addedResourceForeground": "#4CC990",
		"gitDecoration.untrackedResourceForeground": "#4CC990",
		"gitDecoration.deletedResourceForeground": "#FF957B",
		"gitDecoration.conflictingResourceForeground": "#FF957B",
		"dropdown.background": "#282423",
		"dropdown.border": "#4D4848",
		"dropdown.foreground": "#B8B3B2",
		"input.background": "#171212",
		"input.border": "#4D4848",
		"input.foreground": "#B8B3B2",
		"input.placeholderForeground": "#767171",
		"inputOption.activeBorder": "#B970A0",
		"button.background": "#B970A0",
		"button.foreground": "#E7E1E1",
		"button.hoverBackground": "#D086B6",
		"badge.background": "#8C4475",
		"badge.foreground": "#E7E1E1",
		"activityBarBadge.background": "#8C4475",
		"activityBarBadge.foreground": "#E7E1E1",
		"activityBar.background": "#3A3535",
		"activityBar.foreground": "#CFCAC9",
		"activityBar.inactiveForeground": "#767171",
		"statusBar.background": "#3A3535",
		"statusBar.foreground": "#B8B3B2",
		"statusBar.noFolderBackground": "#3A3535",
		"statusBar.noFolderForeground": "#B8B3B2",
		"statusBar.debuggingBackground": "#3A3535",
		"statusBar.debuggingForeground": "#B8B3B2",
		"panel.border": "#4D4848",
		"panelTitle.activeBorder": "#A25A8A",
		"titleBar.activeBackground": "#3A3535",
